# GraphQL schema example
#
# https://gqlgen.com/getting-started/

type User {
  email: String!
  password: String!
}

type Account{
  name: String!
  usern: String!
  password: String!
  data: String!
}

type Query {
  passwords: [User!]!
}

type Mutation{
  creUse(input: User!): Boolean!
  updAcc(input: Account!): Boolean!
}
