# GraphQL schema example
#
# https://gqlgen.com/getting-started/

type User {
  email: String!
  data: String!
}

type Account{
  name: String!
  usern: String!
  password: String!
}

type Query {
  passwords: [User!]!
}

input AccData{
  name: String!
  usern: String!
  password: String!
}

type Mutation{
  creAcc(input: String!): Boolean!
  updAcc(input: String!): Boolean!
  insAcc(input: AccData!): Boolean!
}
